#include <iostream>

using namespace std;

int perfect(int num)
{
    int sum=0;
    for(int i=1;i<num;i++)
    {
        if(num%i==0)
        {
            sum=sum+i;
        }
    }
    if(sum==num)
    {
        cout<<sum;
    }
    else
    cout<<"not";
}
int main()
{
   int num;
   cin>>num;
   perfect(num);
   return 0;
}
