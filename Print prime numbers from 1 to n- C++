#include <iostream>
#include<bits/stdc++.h>

using namespace std;

int isprime(int n)
{
    if(n<=1)
    {
        int temp=0;
        return temp;
    }
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
        {
            int temp=0;
            return temp;
        }
    }
    int temp=1;
    return temp;
}
void print(int n)
{
    for(int i=2;i<=n;i++)
    {
        if(isprime(i)==1)
        {
            cout<<"\n";
            cout<<i;
        }
    }
}
int main()
{
    int n;
    cout<<"Enter the value of n: ";
    cin>>n;
    print(n);
    return 0;
}
