#include <iostream>

using namespace std;

void quadrant(int x,int y)
{
    if(x==0)
    {
        cout<<"y axis";
    }
    else if(y==0)
    {
        cout<<"x axis";
    }
    else if(x>0 && y>0)
    {
        cout<<"1st";
    }
    else if(x<0 && y>0)
    {
        cout<<"2nd";
    }
    else if (x<0 && y<0)
    {
        cout<<"3rd";
    }
    else
    {
        cout<<"4th";
    }
    
}
int main()
{
   int x,y;
   cin>>x>>y;
   quadrant(x,y);
   return 0;
}
